{"ast":null,"code":"import { mapActions } from 'vuex';\nexport default {\n  data() {\n    return {\n      rating: 1,\n      comment: '',\n      skillName: '',\n      targetUser: ''\n    };\n  },\n  methods: {\n    ...mapActions(['rateSkill']),\n    async handleRateSkill() {\n      if (!this.skillName || this.rating < 1 || this.rating > 5) {\n        console.error('Please enter a valid Skill Name and Rating between 1 and 5.');\n        return;\n      }\n      const success = await this.rateSkill([this.skillName, this.rating, this.comment, this.targetUser]);\n      if (success) {\n        console.log('Skill rated successfully!');\n      } else {\n        console.error('Failed to rate skill.');\n      }\n    }\n  },\n  mounted() {\n    let [skillName, targetUser] = this.$route.query.user;\n    this.skillName = skillName;\n    this.targetUser = targetUser;\n  }\n};","map":{"version":3,"sources":["/home/alan/VisualStudioProjects/BU/vue-proj1/src/pages/RateSkillPage.vue"],"names":[],"mappings":"AAmBA,SAAS,UAAS,QAAS,MAAM;AAEjC,eAAe;EACb,IAAI,CAAA,EAAG;IACL,OAAO;MACL,MAAM,EAAE,CAAC;MACT,OAAO,EAAE,EAAE;MACX,SAAS,EAAE,EAAE;MACb,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACD,OAAO,EAAE;IACP,GAAG,UAAU,CAAC,CAAC,WAAW,CAAC,CAAC;IAC5B,MAAM,eAAe,CAAA,EAAG;MACtB,IAAI,CAAC,IAAI,CAAC,SAAQ,IAAK,IAAI,CAAC,MAAK,GAAI,CAAA,IAAK,IAAI,CAAC,MAAK,GAAI,CAAC,EAAE;QACzD,OAAO,CAAC,KAAK,CAAC,6DAA6D,CAAC;QAC5E;MACF;MAEA,MAAM,OAAM,GAAI,MAAM,IAAI,CAAC,SAAS,CAAC,CAAC,IAAI,CAAC,SAAS,EAAE,IAAI,CAAC,MAAM,EAAE,IAAI,CAAC,OAAO,EAAE,IAAI,CAAC,UAAU,CAAC,CAAC;MAElG,IAAI,OAAO,EAAE;QACX,OAAO,CAAC,GAAG,CAAC,2BAA2B,CAAC;MAC1C,CAAA,MAAO;QACL,OAAO,CAAC,KAAK,CAAC,uBAAuB,CAAC;MACxC;IACF;EACF,CAAC;EACD,OAAO,CAAA,EAAG;IACR,IAAI,CAAC,SAAS,EAAE,UAAU,CAAA,GAAI,IAAI,CAAC,MAAM,CAAC,KAAK,CAAC,IAAI;IACpD,IAAI,CAAC,SAAQ,GAAI,SAAS;IAC1B,IAAI,CAAC,UAAS,GAAI,UAAU;EAC9B;AACF,CAAC","sourcesContent":["<template>\n  <div class=\"rate-skill\">\n    <h2>Rate Skill</h2>\n    <div class=\"skill-info\">\n      <label>Skill Name: {{ skillName }}</label>\n      <div class=\"form-group\">\n        <label>Rating (1-5):</label>\n        <input v-model.number=\"rating\" type=\"number\" min=\"1\" max=\"5\" />\n      </div>\n      <div class=\"form-group\">\n        <label>Comment:</label>\n        <input v-model=\"comment\" type=\"text\" placeholder=\"Enter Your Comment\" />\n      </div>\n    </div>\n    <button class=\"rate-button\" @click=\"handleRateSkill\">Rate Skill</button>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from 'vuex';\n\nexport default {\n  data() {\n    return {\n      rating: 1,\n      comment: '',\n      skillName: '',\n      targetUser: ''\n    };\n  },\n  methods: {\n    ...mapActions(['rateSkill']),\n    async handleRateSkill() {\n      if (!this.skillName || this.rating < 1 || this.rating > 5) {\n        console.error('Please enter a valid Skill Name and Rating between 1 and 5.');\n        return;\n      }\n\n      const success = await this.rateSkill([this.skillName, this.rating, this.comment, this.targetUser]);\n\n      if (success) {\n        console.log('Skill rated successfully!');\n      } else {\n        console.error('Failed to rate skill.');\n      }\n    },\n  },\n  mounted() {\n    let [skillName, targetUser] = this.$route.query.user;\n    this.skillName = skillName;\n    this.targetUser = targetUser;\n  },\n};\n</script>\n\n<style scoped>\n.rate-skill {\n  text-align: center;\n}\n\n.skill-info {\n  margin-bottom: 20px;\n}\n\n.form-group {\n  margin-bottom: 10px;\n}\n\nlabel {\n  display: block;\n  font-weight: bold;\n}\n\ninput[type=\"number\"],\ninput[type=\"text\"] {\n  padding: 5px;\n  font-size: 14px;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n}\n\n.rate-button {\n  padding: 10px 20px;\n  font-size: 16px;\n  background-color: #007bff;\n  color: #fff;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n}\n\n.rate-button:hover {\n  background-color: #218838;\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module","externalDependencies":[]}