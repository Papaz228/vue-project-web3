{"ast":null,"code":"import { mapActions } from 'vuex';\nexport default {\n  data() {\n    return {\n      trustRequests: []\n    };\n  },\n  methods: {\n    ...mapActions(['getUserTrustRequests', 'confirmTrustRequest']),\n    async confirmRequest(request) {\n      await this.confirmTrustRequest(request);\n      this.trustRequests = await this.getUserTrustRequests(this.$store.state.userAddress);\n      console.log('Confirmed request:', request);\n    },\n    async loadTrustRequests() {\n      this.trustRequests = await this.getUserTrustRequests(this.$store.state.userAddress);\n    }\n  },\n  async mounted() {\n    await this.loadTrustRequests();\n  }\n};","map":{"version":3,"sources":["/home/alan/VisualStudioProjects/BU/vue-proj1/src/pages/RequestConfirmationPage.vue"],"names":[],"mappings":"AAgBA,SAAS,UAAS,QAAS,MAAM;AAEjC,eAAe;EACb,IAAI,CAAA,EAAG;IACL,OAAO;MACL,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACD,OAAO,EAAE;IACP,GAAG,UAAU,CAAC,CAAC,sBAAsB,EAAE,qBAAqB,CAAC,CAAC;IAC9D,MAAM,cAAc,CAAC,OAAO,EAAE;MAC5B,MAAM,IAAI,CAAC,mBAAmB,CAAC,OAAO,CAAC;MACvC,IAAI,CAAC,aAAY,GAAI,MAAM,IAAI,CAAC,oBAAoB,CAAC,IAAI,CAAC,MAAM,CAAC,KAAK,CAAC,WAAW,CAAC;MACnF,OAAO,CAAC,GAAG,CAAC,oBAAoB,EAAE,OAAO,CAAC;IAC5C,CAAC;IACD,MAAM,iBAAiB,CAAA,EAAG;MACxB,IAAI,CAAC,aAAY,GAAI,MAAM,IAAI,CAAC,oBAAoB,CAAC,IAAI,CAAC,MAAM,CAAC,KAAK,CAAC,WAAW,CAAC;IACrF;EACF,CAAC;EACD,MAAM,OAAO,CAAA,EAAG;IACd,MAAM,IAAI,CAAC,iBAAiB,EAAE;EAChC;AACF,CAAC","sourcesContent":["<template>\n  <div class=\"trust-requests-page\">\n    <h2>Trust Requests Page</h2>\n    <div v-if=\"trustRequests.length > 0\">\n      <div v-for=\"(request, index) in trustRequests\" :key=\"index\" class=\"trust-request\">\n        <p>{{ request }}</p>\n        <button class=\"confirm-button\" @click=\"confirmRequest(request)\">Confirm</button>\n      </div>\n    </div>\n    <div v-else>\n      <h3>No requests found.</h3>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from 'vuex';\n\nexport default {\n  data() {\n    return {\n      trustRequests: [],\n    };\n  },\n  methods: {\n    ...mapActions(['getUserTrustRequests', 'confirmTrustRequest']),\n    async confirmRequest(request) {\n      await this.confirmTrustRequest(request);\n      this.trustRequests = await this.getUserTrustRequests(this.$store.state.userAddress);\n      console.log('Confirmed request:', request);\n    },\n    async loadTrustRequests() {\n      this.trustRequests = await this.getUserTrustRequests(this.$store.state.userAddress);\n    },\n  },\n  async mounted() {\n    await this.loadTrustRequests();\n  },\n};\n</script>\n\n<style scoped>\n.trust-requests-page {\n  text-align: center;\n}\n\n.trust-request {\n  margin: 10px;\n}\n\n.confirm-button {\n  padding: 10px 20px;\n  font-size: 16px;\n  background-color: #28a745;\n  color: #fff;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n}\n\n.confirm-button:hover {\n  background-color: #218838;\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module","externalDependencies":[]}