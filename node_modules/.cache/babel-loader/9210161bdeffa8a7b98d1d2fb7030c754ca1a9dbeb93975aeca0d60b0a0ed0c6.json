{"ast":null,"code":"import { mapActions } from 'vuex';\nexport default {\n  data() {\n    return {\n      username: '',\n      email: ''\n    };\n  },\n  methods: {\n    ...mapActions(['createProfile']),\n    async handleCreateProfile() {\n      if (!this.username || !this.email) {\n        console.error('Please enter both Username and Email.');\n        return;\n      }\n      const profileData = {\n        username: this.username,\n        email: this.email\n      };\n      const success = await this.createProfile(profileData);\n      if (success) {\n        console.log('Profile created successfully!');\n      } else {\n        console.error('Failed to create profile.');\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapActions","data","username","email","methods","handleCreateProfile","console","error","profileData","success","createProfile","log"],"sources":["/home/alan/VisualStudioProjects/BU/vue-proj1/src/components/CreateProfile.vue"],"sourcesContent":["<template>\n    <div>\n      <h2>Create Profile</h2>\n      <label>Username:</label>\n      <input v-model=\"username\" type=\"text\" placeholder=\"Enter Username\" />\n      <label>Email:</label>\n      <input v-model=\"email\" type=\"text\" placeholder=\"Enter Email\" />\n      <button @click=\"handleCreateProfile\">Create Profile</button>\n    </div>\n  </template>\n  \n  <script>\n  import { mapActions } from 'vuex';\n  \n  export default {\n    data() {\n      return {\n        username: '',\n        email: '',\n      };\n    },\n    methods: {\n      ...mapActions(['createProfile']),\n      async handleCreateProfile() {\n        if (!this.username || !this.email) {\n          console.error('Please enter both Username and Email.');\n          return;\n        }\n  \n        const profileData = {\n          username: this.username,\n          email: this.email,\n        };\n  \n        const success = await this.createProfile(profileData);\n  \n        if (success) {\n          console.log('Profile created successfully!');\n        } else {\n          console.error('Failed to create profile.');\n        }\n      },\n    },\n  };\n  </script>\n  "],"mappings":"AAYE,SAASA,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,GAAGJ,UAAU,CAAC,CAAC,eAAe,CAAC,CAAC;IAChC,MAAMK,mBAAmBA,CAAA,EAAG;MAC1B,IAAI,CAAC,IAAI,CAACH,QAAO,IAAK,CAAC,IAAI,CAACC,KAAK,EAAE;QACjCG,OAAO,CAACC,KAAK,CAAC,uCAAuC,CAAC;QACtD;MACF;MAEA,MAAMC,WAAU,GAAI;QAClBN,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,KAAK,EAAE,IAAI,CAACA;MACd,CAAC;MAED,MAAMM,OAAM,GAAI,MAAM,IAAI,CAACC,aAAa,CAACF,WAAW,CAAC;MAErD,IAAIC,OAAO,EAAE;QACXH,OAAO,CAACK,GAAG,CAAC,+BAA+B,CAAC;MAC9C,OAAO;QACLL,OAAO,CAACC,KAAK,CAAC,2BAA2B,CAAC;MAC5C;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}