{"ast":null,"code":"import { mapActions } from 'vuex';\nimport { ethers } from 'ethers';\nexport default {\n  name: 'create-profile',\n  data() {\n    return {\n      name: '',\n      contactInfo: '',\n      socialLinks: ''\n    };\n  },\n  methods: {\n    ...mapActions(['createProfile']),\n    async handleCreateProfile() {\n      if (!this.name || !this.contactInfo) {\n        console.error('Please enter both Name and Contact Information.');\n        return;\n      }\n      const nameBytes = ethers.utils.toUtf8Bytes(this.name);\n      const contactInfoBytes = ethers.utils.toUtf8Bytes(this.contactInfo);\n      const socialLinksBytes = ethers.utils.toUtf8Bytes(this.socialLinks);\n      const success = await this.createProfile(nameBytes, contactInfoBytes, socialLinksBytes);\n      if (success) {\n        console.log('Profile created successfully!');\n      } else {\n        console.error('Failed to create profile.');\n      }\n    }\n  }\n};","map":{"version":3,"sources":["/home/alan/VisualStudioProjects/BU/vue-proj1/src/components/CreateProfile.vue"],"names":[],"mappings":"AAcA,SAAS,UAAS,QAAS,MAAM;AACjC,SAAQ,MAAM,QAAO,QAAQ;AAC7B,eAAe;EACb,IAAI,EAAE,gBAAgB;EACtB,IAAI,CAAA,EAAG;IACL,OAAO;MACL,IAAI,EAAE,EAAE;MACR,WAAW,EAAE,EAAE;MACf,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACD,OAAO,EAAE;IACP,GAAG,UAAU,CAAC,CAAC,eAAe,CAAC,CAAC;IAChC,MAAM,mBAAmB,CAAA,EAAG;MAC9B,IAAI,CAAC,IAAI,CAAC,IAAG,IAAK,CAAC,IAAI,CAAC,WAAW,EAAE;QACnC,OAAO,CAAC,KAAK,CAAC,iDAAiD,CAAC;QAChE;MACF;MAEA,MAAM,SAAQ,GAAI,MAAM,CAAC,KAAK,CAAC,WAAW,CAAC,IAAI,CAAC,IAAI,CAAC;MACrD,MAAM,gBAAe,GAAI,MAAM,CAAC,KAAK,CAAC,WAAW,CAAC,IAAI,CAAC,WAAW,CAAC;MACnE,MAAM,gBAAe,GAAI,MAAM,CAAC,KAAK,CAAC,WAAW,CAAC,IAAI,CAAC,WAAW,CAAC;MAEnE,MAAM,OAAM,GAAI,MAAM,IAAI,CAAC,aAAa,CAAC,SAAS,EAAE,gBAAgB,EAAE,gBAAgB,CAAC;MAEvF,IAAI,OAAO,EAAE;QACX,OAAO,CAAC,GAAG,CAAC,+BAA+B,CAAC;MAC9C,CAAA,MAAO;QACL,OAAO,CAAC,KAAK,CAAC,2BAA2B,CAAC;MAC5C;IACF;EACE;AACF,CAAC","sourcesContent":["<template>\n  <div>\n    <h2>Create Profile</h2>\n    <label>Name:</label>\n    <input v-model=\"name\" type=\"text\" placeholder=\"Enter Name\" />\n    <label>Contact Information:</label>\n    <input v-model=\"contactInfo\" type=\"text\" placeholder=\"Enter Contact Information\" />\n    <label>Social Links:</label>\n    <input v-model=\"socialLinks\" type=\"text\" placeholder=\"Enter Social Links\" />\n    <button @click=\"handleCreateProfile\">Create Profile</button>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from 'vuex';\nimport {ethers} from 'ethers';\nexport default {\n  name: 'create-profile',\n  data() {\n    return {\n      name: '',\n      contactInfo: '',\n      socialLinks: '',\n    };\n  },\n  methods: {\n    ...mapActions(['createProfile']),\n    async handleCreateProfile() {\n  if (!this.name || !this.contactInfo) {\n    console.error('Please enter both Name and Contact Information.');\n    return;\n  }\n\n  const nameBytes = ethers.utils.toUtf8Bytes(this.name);\n  const contactInfoBytes = ethers.utils.toUtf8Bytes(this.contactInfo);\n  const socialLinksBytes = ethers.utils.toUtf8Bytes(this.socialLinks);\n\n  const success = await this.createProfile(nameBytes, contactInfoBytes, socialLinksBytes);\n\n  if (success) {\n    console.log('Profile created successfully!');\n  } else {\n    console.error('Failed to create profile.');\n  }\n},\n  },\n};\n</script>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module","externalDependencies":[]}