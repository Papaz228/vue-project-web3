{"ast":null,"code":"import { mapActions } from 'vuex';\nexport default {\n  name: 'create-profile',\n  data() {\n    return {\n      name: '',\n      contactInfo: '',\n      socialLinks: ''\n    };\n  },\n  methods: {\n    ...mapActions(['createProfile']),\n    async handleCreateProfile() {\n      if (!this.name || !this.contactInfo) {\n        console.error('Please enter both Name and Contact Information.');\n        return;\n      }\n      const profileData = {\n        name: this.name,\n        contactInfo: this.contactInfo,\n        socialLinks: this.socialLinks\n      };\n      const success = await this.createProfile(profileData);\n      if (success) {\n        console.log('Profile created successfully!');\n      } else {\n        console.error('Failed to create profile.');\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapActions","name","data","contactInfo","socialLinks","methods","handleCreateProfile","console","error","profileData","success","createProfile","log"],"sources":["/home/alan/VisualStudioProjects/BU/vue-proj1/src/components/CreateProfile.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>Create Profile</h2>\n    <label>Name:</label>\n    <input v-model=\"name\" type=\"text\" placeholder=\"Enter Name\" />\n    <label>Contact Information:</label>\n    <input v-model=\"contactInfo\" type=\"text\" placeholder=\"Enter Contact Information\" />\n    <label>Social Links:</label>\n    <input v-model=\"socialLinks\" type=\"text\" placeholder=\"Enter Social Links\" />\n    <button @click=\"handleCreateProfile\">Create Profile</button>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from 'vuex';\n\nexport default {\n  name: 'create-profile',\n  data() {\n    return {\n      name: '',\n      contactInfo: '',\n      socialLinks: '',\n    };\n  },\n  methods: {\n    ...mapActions(['createProfile']),\n    async handleCreateProfile() {\n      if (!this.name || !this.contactInfo) {\n        console.error('Please enter both Name and Contact Information.');\n        return;\n      }\n\n      const profileData = {\n        name: this.name,\n        contactInfo: this.contactInfo,\n        socialLinks: this.socialLinks,\n      };\n\n      const success = await this.createProfile(profileData);\n\n      if (success) {\n        console.log('Profile created successfully!');\n      } else {\n        console.error('Failed to create profile.');\n      }\n    },\n  },\n};\n</script>\n"],"mappings":"AAcA,SAASA,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLD,IAAI,EAAE,EAAE;MACRE,WAAW,EAAE,EAAE;MACfC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,GAAGL,UAAU,CAAC,CAAC,eAAe,CAAC,CAAC;IAChC,MAAMM,mBAAmBA,CAAA,EAAG;MAC1B,IAAI,CAAC,IAAI,CAACL,IAAG,IAAK,CAAC,IAAI,CAACE,WAAW,EAAE;QACnCI,OAAO,CAACC,KAAK,CAAC,iDAAiD,CAAC;QAChE;MACF;MAEA,MAAMC,WAAU,GAAI;QAClBR,IAAI,EAAE,IAAI,CAACA,IAAI;QACfE,WAAW,EAAE,IAAI,CAACA,WAAW;QAC7BC,WAAW,EAAE,IAAI,CAACA;MACpB,CAAC;MAED,MAAMM,OAAM,GAAI,MAAM,IAAI,CAACC,aAAa,CAACF,WAAW,CAAC;MAErD,IAAIC,OAAO,EAAE;QACXH,OAAO,CAACK,GAAG,CAAC,+BAA+B,CAAC;MAC9C,OAAO;QACLL,OAAO,CAACC,KAAK,CAAC,2BAA2B,CAAC;MAC5C;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}