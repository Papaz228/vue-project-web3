         };\n          reader.readAsArrayBuffer(this.diplomaFile);\n        } catch (error) {\n          console.error('Error uploading diploma:', error);\n        }\n      },\n      onFileChange(event) {\n        this.diplomaFile = event.target.files[0];\n      },\n    },\n  };\n  </script>\n  "],"sourceRoot":""},"metadata":{},"sourceType":"module","externalDependencies":[]}