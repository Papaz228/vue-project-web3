{"ast":null,"code":"import { mapActions } from 'vuex';\nexport default {\n  data() {\n    return {\n      requestIndex: 0,\n      comment: ''\n    };\n  },\n  methods: {\n    ...mapActions(['confirmSkill']),\n    async handleConfirmSkill() {\n      if (!this.requestIndex) {\n        console.error('Please enter a valid request index.');\n        return;\n      }\n      const success = await this.confirmSkill(this.requestIndex - 1, this.comment);\n      if (success) {\n        console.log('Skill confirmed successfully!');\n      } else {\n        console.error('Failed to confirm skill.');\n      }\n    }\n  }\n};","map":{"version":3,"sources":["/home/alan/VisualStudioProjects/BU/vue-proj1/src/pages/ConfirmSkillPage.vue"],"names":[],"mappings":"AAgBA,SAAS,UAAS,QAAS,MAAM;AAEjC,eAAe;EACb,IAAI,CAAA,EAAG;IACL,OAAO;MACL,YAAY,EAAE,CAAC;MACf,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACD,OAAO,EAAE;IACP,GAAG,UAAU,CAAC,CAAC,cAAc,CAAC,CAAC;IAC/B,MAAM,kBAAkB,CAAA,EAAG;MACzB,IAAI,CAAC,IAAI,CAAC,YAAY,EAAE;QACtB,OAAO,CAAC,KAAK,CAAC,qCAAqC,CAAC;QACpD;MACF;MAEA,MAAM,OAAM,GAAI,MAAM,IAAI,CAAC,YAAY,CAAC,IAAI,CAAC,YAAW,GAAI,CAAC,EAAE,IAAI,CAAC,OAAO,CAAC;MAE5E,IAAI,OAAO,EAAE;QACX,OAAO,CAAC,GAAG,CAAC,+BAA+B,CAAC;MAC9C,CAAA,MAAO;QACL,OAAO,CAAC,KAAK,CAAC,0BAA0B,CAAC;MAC3C;IACF;EACF;AACF,CAAC","sourcesContent":["<template>\n  <div class=\"confirm-skill\">\n    <h2>Confirm Skill</h2>\n    <div class=\"form-group\">\n      <label for=\"request-index\">Request Index:</label>\n      <input v-model.number=\"requestIndex\" id=\"request-index\" type=\"number\" placeholder=\"Enter Request Index\" />\n    </div>\n    <div class=\"form-group\">\n      <label for=\"comment\">Comment:</label>\n      <input v-model=\"comment\" id=\"comment\" type=\"text\" placeholder=\"Enter Comment\" />\n    </div>\n    <button class=\"confirm-button\" @click=\"handleConfirmSkill\">Confirm Skill</button>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from 'vuex';\n\nexport default {\n  data() {\n    return {\n      requestIndex: 0,\n      comment: '',\n    };\n  },\n  methods: {\n    ...mapActions(['confirmSkill']),\n    async handleConfirmSkill() {\n      if (!this.requestIndex) {\n        console.error('Please enter a valid request index.');\n        return;\n      }\n\n      const success = await this.confirmSkill(this.requestIndex - 1, this.comment);\n\n      if (success) {\n        console.log('Skill confirmed successfully!');\n      } else {\n        console.error('Failed to confirm skill.');\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.confirm-skill {\n  text-align: center;\n}\n\n.form-group {\n  margin-bottom: 10px;\n}\n\nlabel {\n  display: block;\n  font-weight: bold;\n}\n\ninput[type=\"number\"],\ninput[type=\"text\"] {\n  padding: 5px;\n  font-size: 14px;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n}\n\n.confirm-button {\n  padding: 10px 20px;\n  font-size: 16px;\n  background-color: #28a745;\n  color: #fff;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n}\n\n.confirm-button:hover {\n  background-color: #218838;\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module","externalDependencies":[]}