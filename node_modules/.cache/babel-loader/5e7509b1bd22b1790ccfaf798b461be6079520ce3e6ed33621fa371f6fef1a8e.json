{"ast":null,"code":"import { mapActions } from 'vuex';\nexport default {\n  data() {\n    return {\n      newSkill: \"\",\n      userSkills: []\n    };\n  },\n  methods: {\n    ...mapActions(['fetchUserProfile', 'fetchAllUserSkills', 'addSkill']),\n    async handleProfileCreated() {\n      await this.fetchUserProfile();\n    },\n    async addSkill() {\n      if (!this.newSkill) {\n        console.error('Please enter a skill.');\n        return;\n      }\n      await this.addSkill(this.newSkill);\n      this.userSkills = await this.fetchAllUserSkills();\n      this.newSkill = '';\n    }\n  },\n  async mounted() {\n    await this.fetchUserProfile();\n  }\n};","map":{"version":3,"sources":["/home/alan/VisualStudioProjects/BU/vue-proj1/src/pages/MyProfilePage.vue"],"names":[],"mappings":"AAmCA,SAAS,UAAS,QAAS,MAAM;AAEjC,eAAe;EACb,IAAI,CAAA,EAAG;IACL,OAAO;MACL,QAAQ,EAAE,EAAE;MACZ,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACD,OAAO,EAAE;IACP,GAAG,UAAU,CAAC,CAAC,kBAAkB,EAAC,oBAAoB,EAAE,UAAU,CAAC,CAAC;IACpE,MAAM,oBAAoB,CAAA,EAAG;MAC3B,MAAM,IAAI,CAAC,gBAAgB,EAAE;IAC/B,CAAC;IACD,MAAM,QAAQ,CAAA,EAAG;MACf,IAAI,CAAC,IAAI,CAAC,QAAQ,EAAE;QAClB,OAAO,CAAC,KAAK,CAAC,uBAAuB,CAAC;QACtC;MACF;MAEA,MAAM,IAAI,CAAC,QAAQ,CAAC,IAAI,CAAC,QAAQ,CAAC;MAClC,IAAI,CAAC,UAAS,GAAI,MAAM,IAAI,CAAC,kBAAkB,EAAC;MAChD,IAAI,CAAC,QAAO,GAAI,EAAE;IACpB;EACF,CAAC;EACD,MAAM,OAAO,CAAA,EAAG;IACd,MAAM,IAAI,CAAC,gBAAgB,EAAE;EAC/B;AACF,CAAC","sourcesContent":["<template>\n  <div>\n    <h2>User Profile</h2>\n    <template v-if=\"$store.state.userProfile && $store.state.userProfile.name && $store.state.userProfile.contactInformation && $store.state.userProfile.socialLinks\">\n      <div>\n        <label>Name:</label>\n        <span>{{ $store.state.userProfile.name }}</span>\n      </div>\n      <div>\n        <label>Contact Information:</label>\n        <span>{{ $store.state.userProfile.contactInformation }}</span>\n      </div>\n      <div>\n        <label>Social Links:</label>\n        <span>{{ $store.state.userProfile.socialLinks }}</span>\n      </div>\n      <div>\n        <label>Add Skill:</label>\n        <input v-model=\"newSkill\" type=\"text\" placeholder=\"Enter Skill\" />\n        <button @click=\"addSkill\">Add Skill</button>\n      </div>\n      <div v-if=\"userSkills.length > 0\">\n        <h3>User Skills:</h3>\n        <ul>\n          <li v-for=\"skill in userSkills\" :key=\"skill\">{{ skill }}</li>\n        </ul>\n      </div>\n    </template>\n    <template v-else>\n      <h3>No profile found.</h3>\n      <create-profile @profile-created=\"handleProfileCreated\" />\n    </template>\n  </div>\n</template>\n<script>\nimport { mapActions } from 'vuex';\n\nexport default {\n  data() {\n    return {\n      newSkill: \"\",\n      userSkills: []\n    };\n  },\n  methods: {\n    ...mapActions(['fetchUserProfile','fetchAllUserSkills', 'addSkill']),\n    async handleProfileCreated() {\n      await this.fetchUserProfile();\n    },\n    async addSkill() {\n      if (!this.newSkill) {\n        console.error('Please enter a skill.');\n        return;\n      }\n\n      await this.addSkill(this.newSkill);\n      this.userSkills = await this.fetchAllUserSkills()\n      this.newSkill = '';\n    },\n  },\n  async mounted() {\n    await this.fetchUserProfile();\n  },\n};\n</script>"],"sourceRoot":""},"metadata":{},"sourceType":"module","externalDependencies":[]}