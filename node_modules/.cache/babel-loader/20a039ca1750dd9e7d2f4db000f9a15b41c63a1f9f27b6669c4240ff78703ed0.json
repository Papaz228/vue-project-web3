{"ast":null,"code":"import { mapActions } from 'vuex';\nexport default {\n  name: 'create-profile',\n  data() {\n    return {\n      name: '',\n      contactInfo: '',\n      socialLinks: ''\n    };\n  },\n  methods: {\n    ...mapActions({\n      createProfile: 'createProfile'\n    }),\n    async handleCreateProfile() {\n      if (!this.name || !this.contactInfo) {\n        console.error('Please enter both Name and Contact Information.');\n        return;\n      }\n      const success = await this.createProfile([this.name, this.contactInfo, this.socialLinks]);\n      if (success) {\n        console.log('Profile created successfully!');\n      } else {\n        console.error('Failed to create profile.');\n      }\n      this.$emit('profile-created');\n    }\n  }\n};","map":{"version":3,"sources":["/home/alan/VisualStudioProjects/BU/vue-proj1/src/components/CreateProfile.vue"],"names":[],"mappings":"AAoBA,SAAS,UAAS,QAAS,MAAM;AAEjC,eAAe;EACb,IAAI,EAAE,gBAAgB;EACtB,IAAI,CAAA,EAAG;IACL,OAAO;MACL,IAAI,EAAE,EAAE;MACR,WAAW,EAAE,EAAE;MACf,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACD,OAAO,EAAE;IACP,GAAG,UAAU,CAAC;MACZ,aAAa,EAAE;IACjB,CAAC,CAAC;IACF,MAAM,mBAAmB,CAAA,EAAG;MAC1B,IAAI,CAAC,IAAI,CAAC,IAAG,IAAK,CAAC,IAAI,CAAC,WAAW,EAAE;QACnC,OAAO,CAAC,KAAK,CAAC,iDAAiD,CAAC;QAChE;MACF;MACA,MAAM,OAAM,GAAI,MAAM,IAAI,CAAC,aAAa,CAAC,CAAC,IAAI,CAAC,IAAI,EAAE,IAAI,CAAC,WAAW,EAAE,IAAI,CAAC,WAAW,CAAC,CAAC;MAEzF,IAAI,OAAO,EAAE;QACX,OAAO,CAAC,GAAG,CAAC,+BAA+B,CAAC;MAC9C,CAAA,MAAO;QACL,OAAO,CAAC,KAAK,CAAC,2BAA2B,CAAC;MAC5C;MACA,IAAI,CAAC,KAAK,CAAC,iBAAiB,CAAC;IAC/B;EACF;AACF,CAAC","sourcesContent":["<template>\n  <div class=\"create-profile\">\n    <h2>Create Profile</h2>\n    <div class=\"form-group\">\n      <label for=\"name\">Name:</label>\n      <input v-model=\"name\" id=\"name\" type=\"text\" placeholder=\"Enter Name\" />\n    </div>\n    <div class=\"form-group\">\n      <label for=\"contact-info\">Contact Information:</label>\n      <input v-model=\"contactInfo\" id=\"contact-info\" type=\"text\" placeholder=\"Enter Contact Information\" />\n    </div>\n    <div class=\"form-group\">\n      <label for=\"social-links\">Social Links:</label>\n      <input v-model=\"socialLinks\" id=\"social-links\" type=\"text\" placeholder=\"Enter Social Links\" />\n    </div>\n    <button class=\"create-profile-button\" @click=\"handleCreateProfile\">Create Profile</button>\n  </div>\n</template>\n\n<script>\nimport { mapActions } from 'vuex';\n\nexport default {\n  name: 'create-profile',\n  data() {\n    return {\n      name: '',\n      contactInfo: '',\n      socialLinks: '',\n    };\n  },\n  methods: {\n    ...mapActions({\n      createProfile: 'createProfile',\n    }),\n    async handleCreateProfile() {\n      if (!this.name || !this.contactInfo) {\n        console.error('Please enter both Name and Contact Information.');\n        return;\n      }\n      const success = await this.createProfile([this.name, this.contactInfo, this.socialLinks]);\n\n      if (success) {\n        console.log('Profile created successfully!');\n      } else {\n        console.error('Failed to create profile.');\n      }\n      this.$emit('profile-created');\n    },\n  },\n};\n</script>\n\n<style scoped>\n.create-profile {\n  text-align: center;\n}\n\n.form-group {\n  margin-bottom: 10px;\n}\n\nlabel {\n  display: block;\n  font-weight: bold;\n}\n\ninput[type=\"text\"] {\n  padding: 5px;\n  font-size: 14px;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n}\n\n.create-profile-button {\n  padding: 10px 20px;\n  font-size: 16px;\n  background-color: #28a745;\n  color: #fff;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n}\n\n.create-profile-button:hover {\n  background-color: #218838;\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module","externalDependencies":[]}